{
  "name": "MiniDBGoClient",
  "version": "1.0.0",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "rsbuild build --config rsbuild.config.ts",
    "start": "serve -s dist -l tcp://0.0.0.0:$PORT -n",
    "preview": "serve -s dist --ssl-cert ./cert.pem --ssl-key ./key.pem -l 3000",
    "--- Biome Scripts ---": "",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint .",
    "check": "npx @biomejs/biome check --apply ."
  },
  "dependencies": {
    "@rsbuild/plugin-svgr": "^1.0.7",
    "clsx": "^1.2.1",
    "lucide-react": "^0.263.1",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-simple-code-editor": "^0.14.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@rsbuild/core": "^1.2.14",
    "@rsbuild/plugin-node-polyfill": "^1.3.0",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rsbuild/plugin-sass": "^1.2.2",
    "@types/node": "^20.0.0",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "resolutions": {
    "react-json-view/react": "^18.2.0",
    "react-json-view/react-dom": "^18.2.0",
    "flux/react": "^18.2.0"
  }
}
